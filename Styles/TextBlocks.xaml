<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style TargetType="TextBlock" x:Key="TextBlockH1" BasedOn="{StaticResource MaterialDesignHeadline3TextBlock}">
        <Setter Property="FontSize" Value="36"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>

    <Style TargetType="TextBlock" x:Key="TextBlockH2" BasedOn="{StaticResource MaterialDesignHeadline3TextBlock}">
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>

    <Style TargetType="TextBlock" x:Key="TextBlockH3" BasedOn="{StaticResource MaterialDesignHeadline3TextBlock}">
        <Setter Property="FontSize" Value="16"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>

    <Style TargetType="TextBlock" x:Key="TextBlockH4" BasedOn="{StaticResource MaterialDesignHeadline3TextBlock}">
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>


</ResourceDictionary>